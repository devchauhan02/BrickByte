generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("MONGO")
}

model User {
    id               String   @id @default(auto()) @map("_id") @db.ObjectId
    name             String?
    email            String?  @unique
    image            String?
    bookedVisits     Json[]
    favResidenciesiD String[] @db.ObjectId
    password         String?
    createdAt        DateTime @default(now())
    updatedAt        DateTime @updatedAt
}

model Residency {
    id           String   @id @default(auto()) @map("_id") @db.ObjectId
    title        String?
    description  String?
    image        String?
    price        Float?
    location     String?
    bookedVisits Json[]
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
}
